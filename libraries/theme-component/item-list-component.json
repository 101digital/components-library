{
  "version":"1.0.0"
  "components": {
    "User Profile Component":{
      "id": "user-profile-component",
      "templates": {
        "User Profile Form Component":{
          "version":"1.0.0",
          "id":"user-profile-form-template",
          "defaultFields":{
            "personalDetails": {
                "label": "Personal details",
                "type": "subtitle"
            },
            "nickName": {
                "label": "Preferred name (will be printed on card)",
                "isEditable": true,
                "type": "textField",
                "rules": ["required", "minLength"],
                "minLength": 3
            },
            "religion": {
                "label": "Religion",
                "isEditable": true,
                "type": "selectField",
                "rules":["required"]
            },
            "email": {
                "label": "Email",
                "isEditable": true,
                "type": "textField",
                "rules": ["required", "email"]
            },
            "maritalStatus": {
                "label": "Marital status",
                "isEditable": true,
                "type": "selectField"
            }
          }
        },
        "User Profile Summary Component":{
          "version":"1.0.0",
          "id":"user-profile-summary-component",
          "defaultFields":{
            "personalDetails": {
                "label": "Personal details",
                "type": "subtitle"
            },
            "nickName": {
                "label": "Preferred name (will be printed on card)",
                "type": "label"
            }
          }
        }
      }
    }
  }
}
